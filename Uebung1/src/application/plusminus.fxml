<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
   <top>
      <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#increaseCounter" text="+" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#decreaseCounter" text="-" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <Label fx:id="number" text="0" underline="true" BorderPane.alignment="CENTER">
         <effect>
            <Glow />
         </effect>
         <font>
            <Font name="Arial Black" size="88.0" />
         </font></Label>
   </center>
</BorderPane>
