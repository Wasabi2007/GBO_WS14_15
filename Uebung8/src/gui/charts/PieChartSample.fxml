<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="gui.charts.PieChartSampleController">
   <children>
      <TableView fx:id="obstTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="obstColum" prefWidth="75.0" text="Obst" />
          <TableColumn fx:id="ammountColum" prefWidth="75.0" text="Menge" />
        </columns>
      </TableView>
      <PieChart fx:id="chart" title="Importiertes Obst" />
      <GridPane fx:id="sliderGrid" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints fillWidth="false" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
        </rowConstraints>
      </GridPane>
   </children>
</VBox>
